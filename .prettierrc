{
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "importOrder": [
        "^react$",
        "^react/(.*)$",
        "^react-dom/(.*)$",
        "^react-$",
        "^@/interfaces/(.*)$",
        "^@/types/(.*)$",
        "^@/app/(.*)$",
        "^@/components/(.*)$",
        "^@/utils/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}
